<Window x:Class="OperationForFolder.RenameFileFilder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OperationForFolder"
        mc:Ignorable="d"
        Title="RenameFileFilder" Height="220" Width="400" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" Margin="30 5 0 0" FontSize="16" Height="21" Width="304" Text="Напишете новое имя файла/каталога" ></TextBlock>
            <TextBlock HorizontalAlignment="Center" Margin="30 0 0 0" FontSize="16" Height="34" Width="304" Text="и нажмите на кнопку переименовать" ></TextBlock>
            <TextBlock x:Name="OwnerTextBlock1" FontSize="16" Height="18" Width="184"></TextBlock>

            <TextBox x:Name="OwnertextBox1"  Margin="0 5 0 0" Width="186" Height="28"/>
            <Button Width="210" Margin="0 10 0 0" Content="переименовать" Height="35" Click="Button_Click" />

        </StackPanel>
    </Grid>
</Window>
